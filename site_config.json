{
    "site_name": "teedoc",
    "site_slogon": "happy to write",
    "site_root_url": "/",
    "site_domain": "teedoc.github.io",
    "site_protocol": "https",
    "config_template_dir": "config",
    "route": {
        "docs": {
            "/Q-Car/zh/": "docs/Q-Car/zh",
            "/Q-Car/en/": "docs/Q-Car/en",
            "/Simple-Foc/zh/": "docs/Simple-Foc/zh",
            "/Simple-Foc/en/": "docs/Simple-Foc/en",
            "/Triode-Car/zh/": "docs/Triode-Car/zh",
            "/Triode-Car/en/": "docs/Triode-Car/en",
            "/About/zh/": "docs/About/zh",
            "/About/en/": "docs/About/en"
        },
        "pages": {
            "/": "pages/index/zh",
            "/en/": "pages/index/en"
        },
        "assets": {
            "/static/": "static",
            "/Q-Car/assets/": "docs/Q-Car/assets"
        },
        "/blog/": "blog"
    },
    "executable": {
        "python": "python3",
        "pip": "pip3"
    },
    "plugins": {
        "teedoc-plugin-markdown-parser":{
            "from": "pypi",
            "config": {
                "toc_depth": 3
            }
        },
        "teedoc-plugin-theme-default":{
            "from": "pypi",
            "config": {
                "dark": true,
                "env":{
                    "main_color": "#4caf7d"
                },
                "css": "/static/css/custom.css",
                "js": "/static/js/custom.js"
            }
        },
        "teedoc-plugin-search":{
            "from": "pypi",
            "config": {
                "search_hint": "Search"
            }
        }
    },
    "rebuild_changes_delay": 0,
    "robots":{
        "User-agent": "*"
    }
}
